{
  "name": "eve",
  "version": "0.0.0",
  "description": "A web application starter",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/djmccormick/eve.git"
  },
  "author": "Dustin McCormick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djmccormick/eve/issues"
  },
  "homepage": "https://github.com/djmccormick/eve#readme",
  "scripts": {
    "lint": "eslint --cache .",
    "format": "prettier --write \"{**/,}*.{js,jsx,ts,mjs,css,less,scss,json,graphql,gql,md,yaml,yml,html,webmanifest,prettierrc,sh}\"",
